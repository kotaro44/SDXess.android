<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginTop="@dimen/sdxess_layout_margin_top"
        android:adjustViewBounds="false"
        android:src="@drawable/title"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right" />

    <TextView
        android:id="@+id/userLbl"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginTop="@dimen/sdxess_layout_margin_top"
        android:text="User"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/userVal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="sdxess3@ji8.net"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userLbl"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginTop="@dimen/sdxess_layout_margin_top"
        android:text="Password"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userVal"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <EditText
        android:id="@+id/passVal"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginTop="0dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView4"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/loginBtn"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left"
        android:layout_marginTop="@dimen/sdxess_layout_margin_top"
        android:text="Log In"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/passVal"
        tools:layout_constraintLeft_creator="1" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/sdxess_layout_margin_bottom"
        android:layout_marginLeft="@dimen/sdxess_layout_margin_left"
        android:text="Client V 1.0.0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginStart="@dimen/sdxess_layout_margin_left" />

    <TextView
        android:id="@+id/signupLbl"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/sdxess_layout_margin_bottom"
        android:layout_marginRight="@dimen/sdxess_layout_margin_right"
        android:text="@string/signup_link"
        android:textAlignment="textEnd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintRight_creator="1"
        android:layout_marginEnd="@dimen/sdxess_layout_margin_right" />

</android.support.constraint.ConstraintLayout>